    "css": "body {\n  font-family: Arial, sans-serif;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n  margin: 0;\n}\n\n.profile-picture-widget {\n  text-align: center;\n}\n\n.profile-picture-container {\n  position: relative;\n  display: inline-block;\n}\n\n.profile-picture {\n  width: 100px;\n  height: 100px;\n  border-radius: 50%;\n  object-fit: cover;\n  cursor: pointer;\n  /* Remove border and background color */\n}\n\n.file-input {\n  display: none;\n}\n\n.upload-button {\n  position: absolute;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  cursor: pointer;\n}\n\n.icon {\n  position: absolute;\n  bottom: 10px;\n  right: 10px;\n  width: 24px;\n  height: 24px;\n  opacity: 0.8;\n  transition: opacity 0.3s ease;\n}\n\n.upload-button:hover .icon {\n  opacity: 1;\n}\n\n.edit-icon {\n  display: none;\n}\n\n.profile-picture[src=\"upload-icon.png\"] + .upload-button .edit-icon {\n  display: none;\n}\n\n.profile-picture:not([src=\"upload-icon.png\"]) + .upload-button .edit-icon {\n  display: block;\n}\n\n.upload-text {\n  text-align: center;\n  margin-top: 10px;\n  font-size: 1rem; /* Set font size to 1rem */\n  color: #333;\n  font-weight: bold; /* Make text bold */\n}",
    "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Profile Picture Widget</title>\n    <link rel=\"stylesheet\" href=\"styles.css\">\n</head>\n<body>\n    <div class=\"profile-picture-widget\">\n        <div class=\"profile-picture-container\">\n            <img id=\"profilePicture\" src=\"data:image/png;base64,\" alt=\"Profile Picture\" class=\"profile-picture\">\n            <input type=\"file\" id=\"fileInput\" accept=\"image/*\" class=\"file-input\">\n            <label for=\"fileInput\" class=\"upload-button\"> </label>\n\t\t\t\t\t<div class=\"upload-text\">Upload Image</div>\n        </div>\n    </div>\n    <script src=\"script.js\"></script>\n</body>\n</html>",
    "js": "appsmith.onReady(() => {\n  const fileInput = document.getElementById('fileInput');\n  const profilePicture = document.getElementById('profilePicture');\n  fileInput.addEventListener('change', function (event) {\n    const file = event.target.files[0];\n    if (file) {\n      const reader = new FileReader();\n      reader.onload = function (e) {\n        profilePicture.src = e.target.result;\n        appsmith.updateModel({\n          image: e.target.result\n        });\n      };\n      reader.readAsDataURL(file);\n    }\n  });\n});"
  },
